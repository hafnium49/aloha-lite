# Environment configuration template
# Copy this to .env and fill in your values

# Phosphobot Configuration
ROBOT_TYPE=simulator
PHOS_URL=http://phosphobot

# Robot Service Configuration
MODEL_ID=phospho-app/so101_dispense_v1-groot
TARGET_POSE=[0.0,1.57,0.0,-1.57,0.0,1.57]
TOL=0.03

# Vision Bridge Configuration
S3_ENDPOINT=http://minio:9000
BUCKET=snapshots

# S3 Credentials (change these in production!)
AWS_ACCESS_KEY_ID=miniokey
AWS_SECRET_ACCESS_KEY=miniosecret

# MinIO Configuration
MINIO_ROOT_USER=miniokey
MINIO_ROOT_PASSWORD=miniosecret
